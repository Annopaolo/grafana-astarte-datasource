{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "lib": ["es6", "dom"],
        "moduleResolution": "node",
        "rootDir": "src/",
        "declaration": false,
        "allowSyntheticDefaultImports": true,
        "inlineSourceMap": false,
        "emitDecoratorMetadata": false,
        "experimentalDecorators": true,
        "noImplicitReturns": true,
        "noImplicitThis": false,
        "noImplicitUseStrict":false,
        "noImplicitAny": false
    },
    "exclude": [
        "node_modules"
    ]
}
